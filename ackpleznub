function whitelist()
game.StarterGui:SetCore("SendNotification", {
    Title = "Whitelisted!";
    Text = "Welcome, "..game.Players.LocalPlayer.Name.."!";
    Duration = 5;
    })
end

function removal()
for l, b in pairs(game.CoreGui:GetChildren()) do
if b.Name == "mainload" then
b:Destroy()
end
end
end

function blacklist()
game.Players.LocalPlayer:Kick("Blacklisted.")
end

lplr = game:GetService("Players").LocalPlayer

---------------
--Whitelisted--
---------------
if lplr.Name == "SinisterPrototype" then
whitelist()
elseif lplr.Name == "incept1cs" then
whitelist()
elseif lplr.Name == "Prox_ity" then
whitelist()
elseif lplr.Name == "SvckDatPpSon" then
whitelist()
elseif lplr.Name == "reinjects" then
whitelist()
